{
  "name": "web-app",
  "version": "1.3.1",
  "license": "BSD-3-Clause-Clear",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"  \"tests/*.{js,jsx,ts,tsx}\" \"tests/tests/**/*.{js,jsx,ts,tsx}\"",
    "license-check": "license-checker --production --onlyAllow 'Apache-2.0; BSD; CC-BY-4.0; ISC; MIT'",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "preview": "vite preview",
    "test": "vite build && vitest",
    "test:ui": "vite build && vitest --ui"
  },
  "dependencies": {
    "@opentdf/client": "file:../lib/opentdf-client-1.3.1.tgz",
    "clsx": "^2.0.0",
    "native-file-system-adapter": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.36.2",
    "@rollup/plugin-inject": "^5.0.3",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "@types/wicg-file-system-access": "^2020.9.6",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitest/ui": "^0.33.0",
    "buffer": "^6.0.3",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "license-checker": "^25.0.1",
    "playwright": "^1.36.2",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vitest": "^0.33.0"
  }
}
